{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "SQL-CONNECTION-STRING"
  },
  "SwaggerSettings": {
    "Title": "v1",
    "Name": "Example Swagger",
    "Enabled": true,
    "Version": "v1",
    "Description": "",
    "Security": {
      "Enabled": true,
      "Name": "example authentication",
      "Scopes": {
        "scope": "description"
      }
    }
  },
  "SecuritySettings": {
    "ApiName": "exemple-webapi",
    "Development": true,
    "Authority": {
      "ClientId": "client",
      "Endpoint": "https://..."
    }
  },
  "CorsSettings": {
    "DefaultPolicy": "_corsPolicy",
    "Policies": [
      {
        "Name": "_corsPolicy",
        "Origins": {
          "ReactHttp": "ORIGIN"
        }
      }
    ]
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Exemplo API",
        "Uri": "/health"
      }
    ]
  }
}
